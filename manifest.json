{
    "name": "Skip fbtracking in links",
    "description": "Allows to copy url without facebook tracker",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "contextMenus", "tabs"
    ],
    "background": {
        "service_worker": "extension.js"
    },
    "icons": {
      "16": "/images/skip-fbclid-16.png",
      "32": "/images/skip-fbclid-32.png",
      "48": "/images/skip-fbclid-48.png",
      "128": "/images/skip-fbclid-128.png"
    }
}